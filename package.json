    "clsx": "^2.1.1",
    "lint-staged": "^15.2.2",
    "lucide-react": "^0.378.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
